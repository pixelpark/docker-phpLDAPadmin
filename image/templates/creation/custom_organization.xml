<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE template SYSTEM "template.dtd">

<template>

	<askcontainer>1</askcontainer>
	<description>New organization</description>
	<icon>ldap-o.png</icon>
	<invalid>0</invalid>
	<rdn>o</rdn>
	<!-- regexp>^$</regexp -->
	<title>Generic: Organization</title>
	<visible>1</visible>

	<objectClasses>
		<objectClass id="organization"></objectClass>
	</objectClasses>

	<attributes>

		<attribute id="o">
			<display>Organization</display>
			<hint>don't include "o="</hint>
			<icon>ldap-o.png</icon>
			<order>1</order>
			<page>1</page>
			<spacer>1</spacer>
		</attribute>

		<attribute id="description">
			<display>Description</display>
			<order>2</order>
			<page>1</page>
			<cols>50</cols>
			<rows>4</rows>
		</attribute>

		<attribute id="businessCategory">
			<display>Business Category</display>
			<order>3</order>
			<page>1</page>
		</attribute>

		<attribute id="postOfficeBox">
			<display>Postoffice box</display>
			<order>4</order>
			<page>1</page>
		</attribute>

		<attribute id="registeredAddress">
			<display>registered Address</display>
			<icon>mail.png</icon>
			<order>5</order>
			<page>1</page>
			<cols>50</cols>
			<rows>4</rows>
			<type>textarea</type>
		</attribute>

		<attribute id="street">
			<display>Street</display>
			<order>6</order>
			<page>1</page>
			<cols>50</cols>
			<rows>4</rows>
			<type>textarea</type>
		</attribute>

		<attribute id="l">
			<display>City</display>
			<order>7</order>
			<page>1</page>
		</attribute>

		<attribute id="st">
			<display>State</display>
			<order>8</order>
			<page>1</page>
		</attribute>

		<attribute id="postalCode">
			<display>Postal code</display>
			<order>9</order>
			<page>1</page>
			<spacer>1</spacer>
		</attribute>

		<attribute id="telephoneNumber">
			<display>Work phone</display>
			<icon>phone.png</icon>
			<order>10</order>
			<page>1</page>
		</attribute>

		<attribute id="facsimileTelephoneNumber">
			<display>Fax</display>
			<order>11</order>
			<page>1</page>
		</attribute>

	</attributes>

</template>
